{
	"compilerOptions": {
		"module": "Node16",
		"target": "ES2022",
		"lib": [
			"ES2022"
		],
		"jsx": "react-jsx",
		"sourceMap": true,
		"rootDir": "src",
		"strict": true,
		"typeRoots": [
            "node_modules/@types",
            "typings"
        ],
		"allowSyntheticDefaultImports": true,
		"esModuleInterop": true,
		"skipLibCheck": true
	},
	"exclude": [
        "node_modules",
        ".vscode-test"
    ]
}
